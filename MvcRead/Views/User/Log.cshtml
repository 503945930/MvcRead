@{
    ViewBag.Title = "Log";
}
<script src="~/Content/js/viewModel/log.js"></script>
<h2>>>日志管理</h2>
<div class="box-content">
    <div class="row">
        <div class="col-md-6">
            <div id="DataTables_Table_0_length"
                class="dataTables_length">
                <label>
                    <select size="1" name="DataTables_Table_0_length" data-bind="options: SelectData, value: ValueData "
                        aria-controls="DataTables_Table_0" id="perPageTotle">
                        @*      <option value="10" selected="selected">10</option>
                        <option value="25">25</option>
                        <option value="50">50</option>
                        <option value="100">100</option>*@
                    </select>
                    每页条数
                </label>
            </div>
        </div>
    </div>
    <table class="table table-striped table-bordered bootstrap-datatable datatable responsive">
        <thead>
            <tr>
                <th>编号</th>
                <th>用户名</th>                      
                <th>操作</th>
                <th>IP</th>    
                <th>操作时间</th>
            </tr>
        </thead>
         <tbody data-bind="template: { name: 'reservationTemplate', foreach: JsonData }">
        </tbody>
    </table>
     <div id="example"></div>
    <input id="totalpage" type="hidden" />
    <script type="text/html" id="reservationTemplate">
        <tr>
            <td class="center"><span data-bind="text: LogID" ></span></td>
            <td class="center"><span data-bind="text: UserName" ></span></td>
            <td class="center"><span data-bind="text: Target"></span></td>
            <td class="center"><span data-bind="text: IP"></span></td>    
            <td class="center"><span data-bind="text: Date"></span></td>       
        </tr>

    </script>
</div>

