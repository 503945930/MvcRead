@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>首页</title>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap-3.2.0/dist/css/bootstrap.min.css" rel="stylesheet" />
